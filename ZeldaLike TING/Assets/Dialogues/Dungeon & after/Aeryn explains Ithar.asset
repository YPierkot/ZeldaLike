  m_Script: {fileID: 11500000, guid: 7df4230d22177bf44a7a35b15fe44399, type: 3}
  m_Name: Aeryn explains Ithar
  m_EditorClassIdentifier: 
  dialogue:
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Pendant que tu cherches la solution, je vais te
      delay: 0
    - expressions: 0
      name: 
      line: "Raconter l'histoire de notre \xEEle."
      delay: 3.5
    - expressions: 0
      name: 
      line: Pendant le Grand Age de la Magie, Ithar connaissait
      delay: 3
    - expressions: 0
      name: 
      line: " et avait cr\xE9\xE9 tous les sorts possibles et imaginables,"
      delay: 3
    - expressions: 0
      name: 
      line: "aussi dangereux ou inutiles qu'ils puissent-\xEAtre."
      delay: 0
    - expressions: 0
      name: 
      line: "Les autres magiciens en \xE9taient amus\xE9s et le laissaient"
      delay: 3
    - expressions: 0
      name: 
      line: tranquille, ils ne le voyaient pas comme une menace,
      delay: 2.5
    - expressions: 0
      name: 
      line: "et Ithar n'en \xE9tait pas une."
      delay: 0
    - expressions: 0
      name: 
      line: "Un jour, il a ouvert une br\xE8che vers la dimension "
      delay: 0
    - expressions: 0
      name: 
      line: "des esprits et a d\xE9chain\xE9 la guerre sur l'\xEEle."
      delay: 0
    - expressions: 0
      name: 
      line: " Les magiciens entr\xE8rent alors en guerre contre lui."
      delay: 0
    - expressions: 0
      name: 
      line: et finirent par l'emprisonner dans la prison de laquelle
      delay: 3
    - expressions: 0
      name: 
      line: " tu l'as lib\xE9r\xE9, prison que je gardais ici, dans mon donjon."
      delay: 3.5
    voiceLine: {fileID: 8300000, guid: e9e149e1dd63a394493db0c99d25d9e7, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "D\xE9sol\xE9 pour \xE7a..."
      delay: 0
    voiceLine: {fileID: 8300000, guid: 87b653a474f545f48a728aab42935b7c, type: 3}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Bref, apr\xE8s \xE7a, les magiciens ont utilis\xE9 le pouvoir"
      delay: 0
    - expressions: 0
      name: 
      line: des Monolithes et de la Rune du Zenith pour lancer
      delay: 2.5
    - expressions: 0
      name: 
      line: "un sort \xE0 l'\xE9chelle de l'\xEEle enti\xE8re pour faire oublier"
      delay: 2.5
    - expressions: 0
      name: 
      line: "\xE0 tout humain l'usage de la magie dangereuse"
      delay: 2.5
    - expressions: 0
      name: 
      line: ' ou de trop grande ampleur.'
      delay: 0
    - expressions: 0
      name: 
      line: " Mes ma\xEEtres aussi ont tout oubli\xE9, donc je ne peux pas "
      delay: 0
    - expressions: 0
      name: 
      line: "plus t'en parler, mais si Ithar a activ\xE9 les Monolithes,"
      delay: 0
    - expressions: 0
      name: 
      line: "c'est qu'il veut lancer un sort \xE0 l'\xE9chelle de l'\xEEle,"
      delay: 3.5
    - expressions: 0
      name: 
      line: "et comme tu peux le comprendre, \xE7a n'est pas bon du tout."
      delay: 3
    voiceLine: {fileID: 8300000, guid: 48a8d9cd9ec39624b90647cc36107a19, type: 3}
