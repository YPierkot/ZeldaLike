  m_Name: Confronting Mist
  m_EditorClassIdentifier: 
  dialogue:
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Mist, tu n'aurais pas quelque chose \xE0 me dire ?"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Comment \xE7a humain?"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Je t'ai trouv\xE9 bien silencieux pendant ma conversation "
      delay: 0
    - expressions: 0
      name: 
      line: avec Aeryn, toi qui es pourtant si bavard.
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Ou veux tu en venir ?
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Je sais que tu me caches quelque chose.
      delay: 0
    - expressions: 0
      name: 
      line: 'Tu sais ce qu''Ithar cherche pas vrai ? '
      delay: 0
    - expressions: 0
      name: 
      line: Tu sais que si tu me le dis, alors Ithar pourra l'extraire
      delay: 0
    - expressions: 0
      name: 
      line: "de ma m\xE9moire et mener son plan \xE0 bien?"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Tu as r\xE9ussi \xE0 r\xE9fl\xE9chir \xE0 tout \xE7a tout seul ?"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Pas le temps pour les blagues Mist, r\xE9ponds moi."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: ' Oui, je sais ce qu''Ithar veut faire.'
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: ' Il serait temps de me mettre au courant '
      delay: 0
    - expressions: 0
      name: 
      line: tu ne penses pas ? Je met ma vie en danger
      delay: 0
    - expressions: 0
      name: 
      line: " j'estime le m\xE9riter."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Je ne suis pas oblig...
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: ' MIST !'
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Bon d'accord, d'accord. Aeryn a vu juste,
      delay: 0
    - expressions: 0
      name: 
      line: "Ithar veut lancer un sort \xE0 l'\xE9chelle de l'\xEEle."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Eh bah enfin ! Quel sort ?
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Ithar n'a jamais visit\xE9 les autres \xEEles, mais la technologie"
      delay: 0
    - expressions: 0
      name: 
      line: des dirigeables est perdue depuis longtemps.
      delay: 0
    - expressions: 0
      name: 
      line: 'Alors il veut utiliser la seule autre chose '
      delay: 0
    - expressions: 0
      name: 
      line: ' qui peut voler pour s''y rendre.'
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "L'\xEEle enti\xE8re ???"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "C'est \xE7a, Ithar veut utiliser l'\xEEle comme moyen de "
      delay: 0
    - expressions: 0
      name: 
      line: "transport, mais cela met en danger l'int\xE9grit\xE9 "
      delay: 0
    - expressions: 0
      name: 
      line: "structurelle de l'\xEEle,  il va consommer \xE9norm\xE9ment de"
      delay: 0
    - expressions: 0
      name: 
      line: "mana de celle-ci. Il y a peu de chance que \xE7a marche"
      delay: 0
    - expressions: 0
      name: 
      line: "mais de grandes chances qu'il d\xE9truise l'\xEEle."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Mais il doit pourtant \xEAtre courant des risques ??"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Ithar n'a que faire des risques humain,
      delay: 0
    - expressions: 0
      name: 
      line: "tant qu'il peut \xE9tendre son savoir."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: " Tu aurais pu me le dire d\xE8s le d\xE9but."
      delay: 0
    - expressions: 0
      name: 
      line: "C'est pour \xE7a que tu t'es cach\xE9 d'Ithar pendant"
      delay: 0
    - expressions: 0
      name: 
      line: votre emprisonnement ?
      delay: 0
    - expressions: 0
      name: 
      line: "Pour pas qu'il d\xE9couvre que tu connais ce sort?"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Oui... C'est des esprits comme moi qu'Ithar cherche,
      delay: 0
    - expressions: 0
      name: 
      line: "c'est pour \xE7a qu'il ouvre de nombreuses br\xE8ches. "
      delay: 0
    - expressions: 0
      name: 
      line: "Je n'ai jamais voulu tout \xE7a moi. "
      delay: 0
    - expressions: 0
      name: 
      line: Je voulais juste vivre une vie tranquille ! Comme avant !
      delay: 0
    - expressions: 0
      name: 
      line: " N'emb\xEAter personne, ne parler \xE0 personne,"
      delay: 0
    - expressions: 0
      name: 
      line: ' juste flotter dans les airs et me laisser porter...'
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 0}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Je te promet qu'apr\xE8s qu'on aie battu Ithar,"
      delay: 0
    - expressions: 0
      name: 
      line: " tu retrouveras la vie dont tu r\xEAves."
      delay: 0
    - expressions: 0
      name: 
      line: "Allez, trouvons Ithar, plus de temps \xE0 perdre."
      delay: 0
    voiceLine: {fileID: 0}
