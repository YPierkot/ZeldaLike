  m_Name: Aeryn explains Ithar
  m_EditorClassIdentifier: 
  dialogue:
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Pendant que tu cherches la solution, je vais te
      delay: 2
    - expressions: 0
      name: 
      line: "Raconter l'histoire de notre \xEEle."
      delay: 2
    voiceLine: {fileID: 8300000, guid: 592da939ef16d6b4b8293b28c60c7790, type: 3}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Pendant le Grand Age de la Magie, '
      delay: 1.5
    - expressions: 0
      name: 
      line: "Ithar connaissait et avait cr\xE9\xE9"
      delay: 2
    - expressions: 0
      name: 
      line: ' tous les sorts possibles et imaginables,'
      delay: 2.5
    - expressions: 0
      name: 
      line: "aussi dangereux ou inutiles qu'ils puissent-\xEAtre."
      delay: 0
    - expressions: 0
      name: 
      line: "Les autres magiciens en \xE9taient "
      delay: 1.5
    - expressions: 0
      name: 
      line: "amus\xE9s et le laissaient tranquille, "
      delay: 1.5
    - expressions: 0
      name: 
      line: ils ne le voyaient pas comme une menace,
      delay: 2.5
    - expressions: 0
      name: 
      line: "et Ithar n'en \xE9tait pas une."
      delay: 0
    - expressions: 0
      name: 
      line: "Un jour, il a ouvert une br\xE8che vers la dimension "
      delay: 0
    - expressions: 0
      name: 
      line: "des esprits et a d\xE9chain\xE9 la guerre sur l'\xEEle."
      delay: 0
    - expressions: 0
      name: 
      line: " Les magiciens entr\xE8rent alors en guerre "
      delay: 0
    - expressions: 0
      name: 
      line: 'contre lui et finirent par l''emprisonner '
      delay: 1.5
    - expressions: 0
      name: 
      line: "dans la prison de laquelle tu l'as lib\xE9r\xE9"
      delay: 1.5
    - expressions: 0
      name: 
      line: prison que je gardais ici, dans mon donjon.
      delay: 3.5
    voiceLine: {fileID: 8300000, guid: 5bcef11b348cd52408e8fef8c04dc02e, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "D\xE9sol\xE9 pour \xE7a..."
      delay: 0
    voiceLine: {fileID: 8300000, guid: 87b653a474f545f48a728aab42935b7c, type: 3}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Bref, apr\xE8s \xE7a, les magiciens ont utilis\xE9 le pouvoir"
      delay: 0
    - expressions: 0
      name: 
      line: 'des Monolithes et de la Rune du Zenith '
      delay: 2.5
    - expressions: 0
      name: 
      line: "pour lancer un sort \xE0 l'\xE9chelle de l'\xEEle "
      delay: 1.5
    - expressions: 0
      name: 
      line: "enti\xE8re pour faire oublier"
      delay: 1.5
    - expressions: 0
      name: 
      line: "\xE0 tout humain l'usage de la magie dangereuse"
      delay: 2.5
    - expressions: 0
      name: 
      line: ' ou de trop grande ampleur.'
      delay: 0
    - expressions: 0
      name: 
      line: "Mes ma\xEEtres aussi ont tout oubli\xE9,"
      delay: 1.5
    - expressions: 0
      name: 
      line: 'donc je ne peux pas plus t''en parler, '
      delay: 1.5
    - expressions: 0
      name: 
      line: "mais si Ithar a activ\xE9 les Monolithes,"
      delay: 1.5
    - expressions: 0
      name: 
      line: "c'est qu'il veut lancer un sort \xE0 l'\xE9chelle de l'\xEEle,"
      delay: 2
    - expressions: 0
      name: 
      line: 'et comme tu peux le comprendre, '
      delay: 1.5
    - expressions: 0
      name: 
      line: "\xE7a n'est pas bon du tout."
      delay: 2
    voiceLine: {fileID: 8300000, guid: 48a8d9cd9ec39624b90647cc36107a19, type: 3}
