MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 0}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: 7df4230d22177bf44a7a35b15fe44399, type: 3}
  m_Name: Aeryn the Wizard
  m_EditorClassIdentifier: 
  dialogue:
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Merci beaucoup de m'avoir lib\xE9r\xE9, mais qui \xEAtes vous ?"
      delay: 0
    - expressions: 0
      name: 
      line: Je ne vous ai jamais vu auparavant.
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Je suis Kell, je viens du village des exil\xE9s."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Je vois, comment \xEAtes vous arriv\xE9 ici ? "
      delay: 0
    - expressions: 0
      name: 
      line: Vous avez combattu les esprits?
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Oui ! Gr\xE2ce \xE0 ces cartes !"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Impressionnant ! Vous avez trouv\xE9 les cartes "
      delay: 0
    - expressions: 0
      name: 
      line: "El\xE9mentaires, vous n'avez donc pas eu besoin "
      delay: 0
    - expressions: 0
      name: 
      line: D'apprendre la magie.
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Oui, c'est ce qu'on m'a dit.
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Par l'esprit qui vous a maudit ?
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Comment le savez v... '
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Enfin, je suis Aeryn, le magicien de glace, et le dernier
      delay: 0
    - expressions: 0
      name: 
      line: "De l'\xEEle, je sais ce que ces marques violettes signifient."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Je suppose que \xE7a fait sens."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Cela vous est arriv\xE9 lorsque vous avez lib\xE9r\xE9 Ithar."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Comment ?? Oui... Vous l'avez vu ?
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Comment pensez vous que j'ai fini dans ce cachot.
      delay: 0
    - expressions: 0
      name: 
      line: "Ce donjon est mon rep\xE8re, Ithar me l'a vol\xE9 pour mener"
      delay: 0
    - expressions: 0
      name: 
      line: "A bien ses petites exp\xE9riences."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Ses exp\xE9riences ?"
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Il essaie de se souvenir d''un sort '
      delay: 0
    - expressions: 0
      name: 
      line: "qu'il a cr\xE9\xE9 avant le grand oubli."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Le grand oubli ?
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Vous ne connaissez vraiment rien de l'histoire de notre \xEEle. "
      delay: 0
    - expressions: 0
      name: 
      line: Pendant le Grand Age de la Magie, Ithar connaissait
      delay: 0
    - expressions: 0
      name: 
      line: " et avait cr\xE9\xE9 tous les sorts possibles et imaginables,"
      delay: 0
    - expressions: 0
      name: 
      line: "aussi dangereux ou inutiles qu'ils puissent-\xEAtre."
      delay: 0
    - expressions: 0
      name: 
      line: "Les autres magiciens en \xE9taient amus\xE9s et le laissaient"
      delay: 0
    - expressions: 0
      name: 
      line: tranquille, ils ne le voyaient pas comme une menace,
      delay: 0
    - expressions: 0
      name: 
      line: "et Ithar n'en \xE9tait pas une."
      delay: 0
    - expressions: 0
      name: 
      line: " Il n'a jamais voulu faire de mal \xE0 qui que ce soit, "
      delay: 0
    - expressions: 0
      name: 
      line: 'mais il s''en fiche de s''il en fait ou non. '
      delay: 0
    - expressions: 0
      name: 
      line: "Un jour, il a ouvert une br\xE8che vers la dimension "
      delay: 0
    - expressions: 0
      name: 
      line: "des esprits et a d\xE9chain\xE9 la guerre sur l'\xEEle."
      delay: 0
    - expressions: 0
      name: 
      line: " Les magiciens entr\xE8rent alors en guerre contre lui."
      delay: 0
    - expressions: 0
      name: 
      line: et finirent par l'emprisonner dans la prison de laquelle
      delay: 0
    - expressions: 0
      name: 
      line: " tu l'as lib\xE9r\xE9, prison que je gardais ici, dans mon donjon."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "D\xE9sol\xE9 pour \xE7a..."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Bref, apr\xE8s \xE7a, les magiciens ont utilis\xE9 le pouvoir"
      delay: 0
    - expressions: 0
      name: 
      line: des Monolithes et de la Rune du Zenith pour lancer
      delay: 0
    - expressions: 0
      name: 
      line: "un sort \xE0 l'\xE9chelle de l'\xEEle enti\xE8re pour faire oublier"
      delay: 0
    - expressions: 0
      name: 
      line: "\xE0 tout humain l'usage de toute magie dangereuse"
      delay: 0
    - expressions: 0
      name: 
      line: ' ou de trop grande ampleur.'
      delay: 0
    - expressions: 0
      name: 
      line: " Mes ma\xEEtres aussi ont tout oubli\xE9, donc je ne peux pas "
      delay: 0
    - expressions: 0
      name: 
      line: "plus t'en parler, mais si Ithar a activ\xE9 les Monolithes,"
      delay: 0
    - expressions: 0
      name: 
      line: "c'est qu'il veut lancer un sort \xE0 l'\xE9chelle de l'\xEEle,"
      delay: 0
    - expressions: 0
      name: 
      line: "et comme tu peux le comprendre, \xE7a n'est pas bon du tout."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Nous devons absolument l'arr\xEAter !"
      delay: 0
    - expressions: 0
      name: 
      line: "Mais je n'ai aucune id\xE9e de comment faire..."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "On doit d\xE9sactiver les Monolithes."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: J'en suis incapable.
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Je sais bien, c'est pour cela que je vais m'en occuper.
      delay: 0
    - expressions: 0
      name: 
      line: "Toi, vas combattre Ithar pr\xE8s du 4\xE8me Monolithe"
      delay: 0
    - expressions: 0
      name: 
      line: "Et donne moi assez de temps pour d\xE9sactiver les autres."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Cela semble assez improvis\xE9 comme plan."
      delay: 0
    voiceLine: {fileID: 0}
  - character: {fileID: 11400000, guid: 300e3e19750b78340a63f80893afcc33, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Mais c''est le seul que nous avons. '
      delay: 0
    - expressions: 0
      name: 
      line: "Tiens prends la derni\xE8re carte, elle pourra t'aider."
      delay: 0
    - expressions: 0
      name: 
      line: "Bonne chance Kell du village des exil\xE9s.  "
      delay: 0
    voiceLine: {fileID: 0}
