  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Shut it mist. Hello! Who\u2019s that on the statue?"
