  dialogue:
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Bonjour ! Vous devez \xEAtre un chercheur du temps?"
      delay: 3
    voiceLine: {fileID: 8300000, guid: 0ad59bedc4d7c4d499daf69ef3137ab5, type: 3}
  - character: {fileID: 11400000, guid: 9de33d469c92bc6409051c0af222d72c, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Oui, mon nom est Remus, '
      delay: 0
    - expressions: 0
      name: 
      line: comment puis-je vous aider voyageur?
      delay: 0
    voiceLine: {fileID: 8300000, guid: 0998766066e6a1941814044a3714f69e, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Je me demandais si vous pouviez me parler '
      delay: 0
    - expressions: 0
      name: 
      line: du Grand Age de la Magie.
      delay: 0
    voiceLine: {fileID: 8300000, guid: 178adb260b20e254f898aa382f544fbd, type: 3}
  - character: {fileID: 11400000, guid: 9de33d469c92bc6409051c0af222d72c, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Oh, vous \xEAtes int\xE9ress\xE9 par nos"
      delay: 0
    - expressions: 0
      name: 
      line: recherches jeune voyageur?
      delay: 0
    - expressions: 0
      name: 
      line: ' C''est peu commun. Eh bien, le Grand Age de la Magie'
      delay: 0
    - expressions: 0
      name: 
      line: "a surtout pris place \xE0 Zenithia, maintenant appel\xE9e Bord de Vent"
      delay: 3.5
    - expressions: 0
      name: 
      line: "Tout a commenc\xE9 lorsque les premiers hommes "
      delay: 0
    - expressions: 0
      name: 
      line: "sont arriv\xE9s sur l'Ile du Zenith et ont remarqu\xE9 "
      delay: 0
    - expressions: 0
      name: 
      line: "d'\xE9tranges ph\xE9nom\xE8nes."
      delay: 0
    - expressions: 0
      name: 
      line: "Certains d'entre eux, les curieux et les malins d\xE9cid\xE8rent"
      delay: 0
    - expressions: 0
      name: 
      line: "qu'ils voulaient en savoir plus sur ces ph\xE9nom\xE8nes"
      delay: 0
    - expressions: 0
      name: 
      line: 'et comprendre comment ils arrivaient '
      delay: 0
    - expressions: 0
      name: 
      line: et ce qu'ils voulaient dire.
      delay: 0
    voiceLine: {fileID: 8300000, guid: dd8af7eb06c4a5e4996389f4265f1730, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "C'\xE9tait de la magie ?\n"
      delay: 0
    voiceLine: {fileID: 8300000, guid: dd2110c9bf25b034cb4eaeb16e84a7d9, type: 3}
  - character: {fileID: 11400000, guid: 9de33d469c92bc6409051c0af222d72c, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Oui, de la magie, dans les arbres, dans l''eau, dans l''air... '
      delay: 3
    - expressions: 0
      name: 
      line: "Partout. Ces hommes furent appel\xE9s les Illumin\xE9s."
      delay: 3
    - expressions: 0
      name: 
      line: "Ils se sont \xE9cart\xE9s des autres habitants pour \xE9tudier"
      delay: 3
    - expressions: 0
      name: 
      line: magie et la comprendre.
      delay: 0
    voiceLine: {fileID: 8300000, guid: 0ff8a3e902ca0c2478ac5d7e765b35ff, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Et apparemment ils ont plut\xF4t bien r\xE9ussi."
      delay: 3
    voiceLine: {fileID: 8300000, guid: e089024383cb94249910b3fb336435f5, type: 3}
  - character: {fileID: 11400000, guid: 9de33d469c92bc6409051c0af222d72c, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Oui en effet. Et quand ils sont revenus avec la capacit\xE9"
      delay: 3
    - expressions: 0
      name: 
      line: "de la contr\xF4ler, ils ont fascin\xE9 le peuple et ont d\xE9cid\xE9"
      delay: 3
    - expressions: 0
      name: 
      line: "que tous devaient y avoir acc\xE8s. C'est l\xE0 qu'ils ont "
      delay: 3
    - expressions: 0
      name: 
      line: "fond\xE9 Z\xE9nithia, capitale de la magie."
      delay: 3
    voiceLine: {fileID: 8300000, guid: c2ef8fb07b3ff804c848eb2c21be42a2, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Wow, c'est impressionnant. Merci pour cette histoire.
      delay: 3
    voiceLine: {fileID: 8300000, guid: e2257b73d67322647a38c7a7e1e7f700, type: 3}
  - character: {fileID: 11400000, guid: 028d51600b7707c48964a837ca1e30f2, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Ces gens font vraiment bien leur travail.
      delay: 0
    voiceLine: {fileID: 8300000, guid: 4dae2746c8ec7cb47967e0723964c718, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Eh moi aussi je fais bien mon travail.
      delay: 2.5
    voiceLine: {fileID: 8300000, guid: 0766fe27cf3fedc4bb5f8aa059ca6196, type: 3}
  - character: {fileID: 11400000, guid: 028d51600b7707c48964a837ca1e30f2, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Tu voles, ce n'est pas ce que j'appelle un travail.
      delay: 2.5
    voiceLine: {fileID: 8300000, guid: 6f182f8179ca8614cbb182d450cb7017, type: 3}
