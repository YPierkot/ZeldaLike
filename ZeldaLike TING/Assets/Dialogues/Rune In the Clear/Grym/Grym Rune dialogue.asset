  m_Name: Grym Rune dialogue
  m_EditorClassIdentifier: 
  dialogue:
  - character: {fileID: 11400000, guid: 028d51600b7707c48964a837ca1e30f2, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 2
      name: 
      line: Tu vas vraiment aller lui parler ?
      delay: 0
    - expressions: 2
      name: 
      line: "Tu es curieux, \xEAtre de chair, "
      delay: 2
    - expressions: 2
      name: 
      line: "je croyais que la magie ne t'int\xE9ressait pas "
      delay: 1.5
    voiceLine: {fileID: 8300000, guid: 994888c9e4056ef4b848ff25dce0bfe7, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Enfin Mist, tu ne fais que te plaindre que je sois ignorant.
      delay: 3.5
    - expressions: 0
      name: 
      line: Laisse moi apprendre des choses !
      delay: 1.75
    - expressions: 0
      name: 
      line: 'Bonjour ! Fynn m''a dit que vous pouviez me parler '
      delay: 3.5
    - expressions: 0
      name: 
      line: De cette fresque !
      delay: 1
    voiceLine: {fileID: 8300000, guid: d05f433a8b2143c4b9aac4ee7a4232cc, type: 3}
  - character: {fileID: 11400000, guid: 9bf4bad436818a04d893ec83fa355436, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: Oui bien sur ! Avec plaisir !
      delay: 0
    - expressions: 0
      name: 
      line: 'Je m''appelle Grym ! '
      delay: 1.5
    voiceLine: {fileID: 8300000, guid: 77c6b37f0a77a9e45be4ad7b6a460d6d, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Enchant\xE9."
      delay: 1.25
    voiceLine: {fileID: 8300000, guid: 69c4e7b5ea402f14b9fef56b4d27f4b0, type: 3}
  - character: {fileID: 11400000, guid: 9bf4bad436818a04d893ec83fa355436, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Alors, cette fresque parle de la Rune du Z\xE9nith et de "
      delay: 3
    - expressions: 0
      name: 
      line: "Comment cette derni\xE8re fut utilis\xE9e"
      delay: 0
    - expressions: 0
      name: 
      line: pendant le Grand Age de la Magie
      delay: 2
    - expressions: 0
      name: 
      line: "On peut voir que d'une mani\xE8re ou d'une autre "
      delay: 2
    - expressions: 0
      name: 
      line: "elle est li\xE9e aux Monolithes."
      delay: 2
    - expressions: 0
      name: 
      line: Et un sort semble en sortir.
      delay: 3
    - expressions: 0
      name: 
      line: 'Je suis venu ici car la rumeur '
      delay: 3.5
    - expressions: 0
      name: 
      line: "disait que la rune commen\xE7ait \xE0 briller"
      delay: 1.5
    - expressions: 0
      name: 
      line: Et pour une fois la rumeur dit vrai !
      delay: 3
    voiceLine: {fileID: 8300000, guid: fad2d5efee033084e901a4274d313091, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 2
      name: 
      line: Le Grand Age de la Magie ?
      delay: 1.75
    voiceLine: {fileID: 8300000, guid: 0d56636366b864541a0300af70bb8393, type: 3}
  - character: {fileID: 11400000, guid: 9bf4bad436818a04d893ec83fa355436, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Mais oui voyons ! C'\xE9tait il y longtemps, "
      delay: 2
    - expressions: 0
      name: 
      line: 'Quand nous savions comment utiliser '
      delay: 2
    - expressions: 0
      name: 
      line: "La Rune du Z\xE9nith ou les Monolithes, quand tout le monde"
      delay: 3.5
    - expressions: 0
      name: 
      line: "Pouvait lancer d'\xE9normes sorts !"
      delay: 0
    - expressions: 0
      name: 
      line: "L'humanit\xE9 \xE9tait \xE0 son glas !"
      delay: 0
    - expressions: 0
      name: 
      line: "Enfin bref, dans tous les cas si vous allez \xE0 la Montagne Venteuse"
      delay: 3
    - expressions: 0
      name: 
      line: "Vous passerez \xE0 travers les ruines de Zenithia"
      delay: 3.5
    - expressions: 0
      name: 
      line: "La capitale de notre fi\xE8re \xEEle \xE0 l'\xE9poque !"
      delay: 3
    voiceLine: {fileID: 8300000, guid: 4378b8884d4db8e4ebc53cc8ae499e1f, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Eh bien merci pour toutes ces informations ! '
      delay: 0
    - expressions: 0
      name: 
      line: Je vous souhaite bonne chance dans vos recherches !
      delay: 0
    - expressions: 0
      name: 
      line: "Eh Mist ? C'est de cette \xE9poque que tu me parles ?"
      delay: 3.5
    - expressions: 0
      name: 
      line: "Quand les humaisn \xE9taient... Braves ?"
      delay: 0
    voiceLine: {fileID: 8300000, guid: 5a47c2995939bdf438eec3475c3b78b9, type: 3}
  - character: {fileID: 11400000, guid: 028d51600b7707c48964a837ca1e30f2, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Oui, et c'\xE9tait toujours le cas quand "
      delay: 2
    - expressions: 0
      name: 
      line: "j'ai \xE9t\xE9 emprisonn\xE9 avec Ithar"
      delay: 1.5
    voiceLine: {fileID: 8300000, guid: 4879c18f707d4f44eae829064c2500d3, type: 3}
  - character: {fileID: 11400000, guid: 34b62f467344d6b4b98708ca1e13ca9e, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: "Donc tu ne sais pas ce qui est arriv\xE9 apr\xE8s ?"
      delay: 0
    voiceLine: {fileID: 8300000, guid: 21e82dd37809d9540a44af85da612851, type: 3}
  - character: {fileID: 11400000, guid: 028d51600b7707c48964a837ca1e30f2, type: 2}
    startingDelay: 0
    dialogLines:
    - expressions: 0
      name: 
      line: 'Non... Mais je suis sur qu''un de ces '
      delay: 0
    - expressions: 0
      name: 
      line: "Int\xE9ressants petits humains te le dira avec le sourire."
      delay: 3.5
    voiceLine: {fileID: 8300000, guid: 540d1f39383d924418e09b4777604bd1, type: 3}
